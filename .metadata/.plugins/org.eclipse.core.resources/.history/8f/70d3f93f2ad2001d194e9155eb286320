package freedomgame;

public enum Weapon {
	LONGSORD("LongSord","100","5"),
	BFSORD("BFSord","500","15"),
	STEELSORD("SteelSord","300","30"),
	LIGHTSORD("LightSord","10000G","공격력 50");
	
	public String weapon;
	Weapon(String weapon){
		this.weapon = weapon;
	}
	public String getName() {
		return weapon;
	}
}
