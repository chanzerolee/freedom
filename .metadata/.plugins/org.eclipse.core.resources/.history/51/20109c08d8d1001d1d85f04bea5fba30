package freedomgame;
import java.util.HashMap;

public class User {
	String name;
	int money;
	int item;
	HashMap<HashMap<String,Integer>, Integer> weapon = new HashMap<>();
	public User(int money, int item) {
		this.money = money;
		this.item = item;
	}
	public void setWeaepon() {
		this.weapon.keySet();
	}
	public void getBag() {
		System.out.println("돈 : "+this.money);
		System.out.println("무기 : "+this.weapon);
	}
}